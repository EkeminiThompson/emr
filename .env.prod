# Azure PostgreSQL Database Configuration
AZURE_POSTGRES_CONNECTION_STRING=postgresql://${USERNAME}@${SERVER_NAME}:${PASSWORD}@${SERVER_NAME}.postgres.database.azure.com/${DATABASE_NAME}?sslmode=require

# FastAPI Configuration
SECRET_KEY=your_strong_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440
LOG_LEVEL=info

# SMTP Email Configuration (if needed)
SMTP_SERVER=smtp.yourprovider.com
SMTP_PORT=587
SMTP_USER=your_email@domain.com
SMTP_PASSWORD=your_email_password

# Frontend Configuration
REACT_APP_API_URL=/api
REACT_APP_NODE_API_URL=/chat